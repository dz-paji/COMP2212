{
  "name": "tsl-highlight",
  "displayName": "Tsl-Highlight",
  "description": "Highlight keywords for COMP2212 dsl",
  "publisher": "paji",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tsl",
      "aliases": ["Tsl", "tsl"],
      "extensions": [".tsl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tsl",
      "scopeName": "source.tsl",
      "path": "./syntaxes/tsl.tmLanguage.json"
    }]
  }
}
